// RUN: run-pass
// EXIT: 42
// SPEC: 5.2.6
// DESC: Match on enum values

enum Status {
    Ok = 0
    Error = 1
    Pending = 2
}

procedure status_value(s: Status) -> i32 {
    return match s {
        Status::Ok => { 42 }
        Status::Error => { 0 }
        Status::Pending => { 1 }
    }
}

public procedure main(move ctx: Context) -> i32 {
    let s = Status::Ok
    return status_value(s)
}
