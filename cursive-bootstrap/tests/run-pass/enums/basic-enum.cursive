// RUN: run-pass
// EXIT: 1
// SPEC: 5.10
// DESC: Basic enum declaration and matching

enum Color {
    Red
    Green
    Blue
}

public procedure main(move ctx: Context) -> i32 {
    let c: Color = Color::Green
    let result: i32 = match c {
        Color::Red => { 0 }
        Color::Green => { 1 }
        Color::Blue => { 2 }
    }
    return result
}
