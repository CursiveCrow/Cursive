// RUN: run-pass
// EXIT: 42
// SPEC: 5.2.6
// DESC: Early return from procedure

procedure find_first_positive(arr: [i32; 5]) -> i32 {
    loop i in 0..5 {
        if arr[i] > 0 {
            return arr[i]
        }
    }
    return 0
}

public procedure main(move ctx: Context) -> i32 {
    let arr: [i32; 5] = [-1, -2, 42, 100, 200]
    return find_first_positive(arr)
}
