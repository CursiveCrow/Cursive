// RUN: run-pass
// EXIT: 42
// SPEC: 5.2.6
// DESC: Nested loops with labeled break
// SKIP: Labeled loops not in Cursive0 spec (full Cursive feature)

public procedure main(move ctx: Context) -> i32 {
    var sum: i32 = 0
    var i: i32 = 0
    'outer: loop i < 10 {
        var j: i32 = 0
        loop j < 10 {
            sum = sum + 1
            if sum >= 42 {
                break 'outer
            }
            j = j + 1
        }
        i = i + 1
    }
    return sum
}
