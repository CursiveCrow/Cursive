// RUN: run-pass
// EXIT: 42
// SPEC: 5.2.6
// DESC: Match with range pattern

procedure classify(n: i32) -> i32 {
    return match n {
        0..10 => { 10 }
        10..50 => { 42 }
        _ => { 100 }
    }
}

public procedure main(move ctx: Context) -> i32 {
    let n = 25
    return classify(n)
}
