// RUN: run-pass
// EXIT: 42
// SPEC: 5.2.6
// DESC: Array indexing and element extraction

procedure sum_first_three(arr: [i32; 5]) -> i32 {
    let a: i32 = arr[0usize]
    let b: i32 = arr[1usize]
    let c: i32 = arr[2usize]
    return a + b + c
}

public procedure main(move ctx: Context) -> i32 {
    let arr: [i32; 5] = [10, 12, 20, 0, 0]
    return sum_first_three(arr)
}
