// RUN: run-pass
// EXIT: 0
// SPEC: 5.2.5
// DESC: Comparison operators

public procedure main(move ctx: Context) -> i32 {
    let a: i32 = 10
    let b: i32 = 20
    let eq: bool = a == a
    let neq: bool = a != b
    let lt: bool = a < b
    let le: bool = a <= b
    let gt: bool = b > a
    let ge: bool = b >= a
    return 0
}
