class Printable {
    procedure value(~) -> i32
}

record Number <: Printable {
    n: i32
    procedure value(~) -> i32 {
        return ~.n
    }
}

public procedure main(move ctx: Context) -> i32 {
    let num: Number = Number { n: 42 }
    return num ~> value()
}
