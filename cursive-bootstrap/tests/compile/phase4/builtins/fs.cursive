public procedure main(ctx: Context) -> i32 {
  let raw: *imm i32 = null;
  let fs: $FileSystem = unsafe {
    transmute::<(*imm i32, *imm i32), $FileSystem>((raw, raw))
  };
  let s: string@View = "hi";
  fs~>create_dir(s);
  let _e: bool = fs~>exists(s);
  return 0;
}
