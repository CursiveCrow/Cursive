# SPEC_COV: ABI-Ret-ByRef
# SPEC_COV: ABI-Call

record SmallRecord { x: i32, y: i32 }
record LargeRecord { a: u64, b: u64, c: u64 }

procedure take_small(move s: SmallRecord) -> i32 { 0 }
procedure borrow_small(s: SmallRecord) -> i32 { 0 }
procedure take_large(move l: LargeRecord) -> u64 { 0u64 }
procedure make_small() -> SmallRecord { SmallRecord { x: 1, y: 2 } }
procedure make_large() -> LargeRecord { LargeRecord { a: 1, b: 2, c: 3 } }

public procedure main(ctx: Context) -> i32 {
  let s = SmallRecord { x: 10, y: 20 };
  let l = LargeRecord { a: 1, b: 2, c: 3 };
  let _ = borrow_small(s);
  let _ = take_small(move s);
  let _ = take_large(move l);
  let s2 = make_small();
  let l2 = make_large();
  0
}
