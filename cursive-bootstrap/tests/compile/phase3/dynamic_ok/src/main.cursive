# SPEC_COV: T-Dynamic-Form
# SPEC_COV: T-Dynamic-MethodCall
# SPEC_COV: T-MethodCall
class D { procedure ping(~) -> i32; }
record R <: D { v: i32, procedure ping(~) -> i32 { return 1; } }

public procedure main(ctx: Context) -> i32 {
  let r = R { v: 0 };
  let x = r~>ping();
  let d: $D = r as $D;
  return d~>ping();
}
