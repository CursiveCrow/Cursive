# SPEC_COV: Move-IntoRecord
# SPEC_COV: B-Move-Whole
// Move value into record field during construction.
// Tests that moved values can be used in record initialization.

record Container {
  val: i32
}

public procedure main(ctx: Context) -> i32 {
  let x: i32 = 42;
  let c: Container = Container { val: move x };
  return c.val;
}
