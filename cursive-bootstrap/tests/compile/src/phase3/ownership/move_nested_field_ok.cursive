# SPEC_COV: Move-NestedField
# SPEC_COV: B-Move-Field
// Move from nested field (outer.inner.field) with unique permission.
// Tests move semantics through nested record access.

record Inner {
  val: i32
}

record Outer {
  inner: Inner
}

public procedure main(ctx: Context) -> i32 {
  let o: unique Outer = unsafe { transmute::<Outer, unique Outer>(Outer { inner: Inner { val: 42 } }) };
  let v: unique i32 = move o.inner.val;
  return 0;
}
