# SPEC_COV: C0X-8.3-OverrideConcrete
# SPEC_COV: Override-Concrete
// ยง8.3: Overriding a concrete class procedure: override MUST be used.

class WithDefault {
  procedure value(~) -> i32 { return 0 }
}

record Custom <: WithDefault {
  v: i32
  override procedure value(~) -> i32 { return self.v }
}

public procedure main(move ctx: Context) -> i32 {
  var c: Custom = Custom { v: 42 }
  return c ~> value()
}
