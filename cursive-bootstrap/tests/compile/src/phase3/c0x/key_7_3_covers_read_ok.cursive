record Data { x: i32, y: i32 }

public procedure main(ctx: Context) -> i32 {
  var d: shared Data = Data { x: 1, y: 2 }
  # d {
    // Key (d, Read) covers (d.x, Read) because d is prefix of d.x
    return d.x + d.y
  }
  return 0
}
