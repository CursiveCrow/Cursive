# SPEC_COV: C0X-Contract-AtEntry
# SPEC_COV: C0X-Contract-EntryCapture
// Tests @entry() intrinsic for capturing entry-state values.

procedure increment(x: unique i32) -> i32
  |= => @result == @entry(x) + 1
{
  x = x + 1
  return x
}

public procedure main(ctx: Context) -> i32 {
  var val: i32 = 10
  return increment(val)
}
