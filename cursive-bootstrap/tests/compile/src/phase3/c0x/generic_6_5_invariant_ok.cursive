record Mutable<T> { value: T }

// Mutable<A> is invariant in T (both read and write positions)

public procedure main(ctx: Context) -> i32 {
  var m: Mutable<i32> = Mutable<i32> { value: 42 }
  m.value = 100
  return m.value
}
