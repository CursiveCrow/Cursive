# SPEC_COV: C0X-Key-MultipleMarkerErr
# SPEC_COV: E-CON-0003
// Tests that multiple # markers in one path is an error.
// Expected diagnostic: E-CON-0003

record Inner { val: i32 }
record Outer { inner: Inner }

public procedure main(ctx: Context) -> i32 {
  var o: shared Outer = Outer { inner: Inner { val: 42 } }
  # o.#inner.#val {
    // Error: multiple # markers
    return o.inner.val
  }
  return 0
}
