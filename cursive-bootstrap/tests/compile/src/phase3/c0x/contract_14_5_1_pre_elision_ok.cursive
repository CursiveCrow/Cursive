# SPEC_COV: C0X-14.5.1-PreElision
# SPEC_COV: Pre-Elision
// ยง14.5.1: |= P means pre=P, post=true. |= => Q means pre=true.

// pre = x > 0, post = true
procedure pre_only(x: i32) -> i32
  |= x > 0
{
  return x
}

// pre = true, post = @result >= 0
procedure post_only(x: i32) -> i32
  |= => @result >= 0
{
  if x < 0 { return 0 - x }
  return x
}

public procedure main(ctx: Context) -> i32 {
  var a: i32 = 5
  return pre_only(a)
}
