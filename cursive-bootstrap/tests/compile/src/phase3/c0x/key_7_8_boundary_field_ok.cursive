# SPEC_COV: TypeLevel-Boundary
// ยง7.8: Field marked with # is type-level boundary. Key path stops there.

record Protected { #inner: ProtectedInner }
record ProtectedInner { value: i32 }

public procedure main(ctx: Context) -> i32 {
  var p: shared Protected = Protected { inner: ProtectedInner { value: 42 } }
  // Key path stops at #inner boundary
  # p.inner {
    return p.inner.value
  }
  return 0
}
