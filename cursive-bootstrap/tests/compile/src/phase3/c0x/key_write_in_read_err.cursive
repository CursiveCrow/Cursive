# SPEC_COV: C0X-Key-WriteInReadErr
# SPEC_COV: E-CON-0070
// Tests that writing in read-mode key block is an error.
// Expected diagnostic: E-CON-0070

record Data { value: i32 }

public procedure main(ctx: Context) -> i32 {
  var d: shared Data = Data { value: 42 }
  # d {
    // Error: write operation in read-mode block
    d.value = 100
  }
  return d.value
}
