# SPEC_COV: C0X-Key-DisjointIndices
# SPEC_COV: C0X-Key-IndexDisjointness
// Tests that keys to statically different indices are disjoint.

public procedure main(ctx: Context) -> i32 {
  var arr: shared [i32; 3] = [1, 2, 3]
  # arr[0] write {
    # arr[1] write {
      // Keys to arr[0] and arr[1] are disjoint
      arr[0] = 10
      arr[1] = 20
    }
  }
  return arr[0] + arr[1]
}
