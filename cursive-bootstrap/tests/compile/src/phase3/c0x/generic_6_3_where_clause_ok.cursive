# SPEC_COV: C0X-6.3-WhereClause
# SPEC_COV: Where-WF
// ยง6.3: where clause introduces bounds on type parameters.

class Printable {
  procedure print(~) -> i32
}

record MyPrintable <: Printable {
  v: i32
  procedure print(~) -> i32 {
    return self.v
  }
}

record Wrapper<T> where T <: Printable { value: T }

public procedure main(move ctx: Context) -> i32 {
  var w: Wrapper<MyPrintable> = Wrapper<MyPrintable> { value: MyPrintable { v: 42 } }
  return w.value ~> print()
}
