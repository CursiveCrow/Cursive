# SPEC_COV: C0X-14.5.4-Entry
# SPEC_COV: Entry-Type
// ยง14.5.4: @entry(expr) evaluates expr in entry state.
// Result type MUST implement Bitcopy or Clone.

procedure increment_check(x: i32) -> i32
  |= => @result == @entry(x) + 1
{
  return x + 1
}

public procedure main(ctx: Context) -> i32 {
  var a: i32 = 10
  return increment_check(a)
}
