# SPEC_COV: C0X-Capture-Unique-Move
# SPEC_COV: ยง18.3.3
// Tests unique binding capture with explicit move (ยง18.3.3)
// Expected: no diagnostics

record Data { value: i32 }

public procedure main(ctx: Context) -> i32 {
  let domain: $ExecutionDomain = ctx~>cpu()
  let data: unique Data = Data { value: 42 }
  parallel domain {
    let h = spawn [move data] {
      // OK: unique binding moved into spawn
      data.value
    }
    wait h
  }
  return 0
}
