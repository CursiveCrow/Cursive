# SPEC_COV: C0X-Key-Speculative-NoNested
# SPEC_COV: E-CON-0090
// Tests that nested key blocks inside speculative block is an error.
// Expected diagnostic: E-CON-0090

record Data { value: i32 }

public procedure main(ctx: Context) -> i32 {
  var d: shared Data = Data { value: 42 }
  # d speculative write {
    # d.value {
      // Error: nested key block in speculative
    }
  }
  return d.value
}
