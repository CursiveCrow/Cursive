# SPEC_COV: C0X-Contract-Full
# SPEC_COV: C0X-Contract-Precondition
# SPEC_COV: C0X-Contract-Postcondition
// Tests full contract with both pre and postcondition.

procedure bounded_increment(x: i32, max: i32) -> i32
  |= x >= 0 && x < max => @result > x && @result <= max
{
  return x + 1
}

public procedure main(ctx: Context) -> i32 {
  var val: i32 = 5
  var limit: i32 = 100
  return bounded_increment(val, limit)
}
