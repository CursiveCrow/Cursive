# SPEC_COV: Perm-Array-Element
# SPEC_COV: P-Array-Perm-Propagate
// Permission propagates to array elements.
// var binding allows mutation of array elements.

public procedure main(ctx: Context) -> i32 {
  var arr: [i32; 3] = [1, 2, 3];
  arr[0usize] = 10;
  arr[1usize] = 20;
  return arr[0usize] + arr[1usize];
}
