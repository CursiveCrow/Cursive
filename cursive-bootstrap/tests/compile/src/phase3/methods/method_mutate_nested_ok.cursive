# SPEC_COV: Method-MutateNested
# SPEC_COV: T-Field
# SPEC_COV: Recv-Unique
// Method returning computed value from fields.
// Tests binary operations on field accesses.

record Pair {
  x: i32,
  y: i32,
  procedure diff(~) -> i32 { return self.x - self.y; }
}

public procedure main(ctx: Context) -> i32 {
  let p = Pair { x: 10, y: 3 };
  return p~>diff();
}
