# SPEC_COV: Method-ReturnField
# SPEC_COV: T-Field
// Method that returns self.field on a non-Bitcopy record.
// This exercises the place typing path for field access.

record Data {
  value: i32,
  procedure get_value(~) -> i32 { return self.value; }
}

public procedure main(ctx: Context) -> i32 {
  let d = Data { value: 42 };
  return d~>get_value();
}
