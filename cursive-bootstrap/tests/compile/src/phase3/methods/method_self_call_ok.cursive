# SPEC_COV: Method-SelfCall
# SPEC_COV: T-MethodCall
// Tests multiple method calls on same receiver.

record Calculator {
  x: i32,
  y: i32,
  procedure get_x(~) -> i32 { return self.x; },
  procedure get_y(~) -> i32 { return self.y; }
}

public procedure main(ctx: Context) -> i32 {
  let c = Calculator { x: 3, y: 7 };
  return c~>get_x() + c~>get_y();
}
