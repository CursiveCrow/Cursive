# SPEC_COV: Method-TempReceiver
# SPEC_COV: T-MethodCall
// Method call with record values as receivers.
// Tests method dispatch on multiple records.

record Wrapper {
  val: i32,
  procedure get(~) -> i32 { return self.val; }
}

public procedure main(ctx: Context) -> i32 {
  let w1 = Wrapper { val: 10 };
  let w2 = Wrapper { val: 21 };
  return w1~>get() + w2~>get();
}
