# SPEC_COV: Method-Chaining
# SPEC_COV: T-MethodCall
// Method with arguments.
// Tests method calls with non-move arguments.

record Builder {
  val: i32,
  procedure use_arg(~, x: i32, y: i32) -> i32 { return x + y; }
}

public procedure main(ctx: Context) -> i32 {
  let b = Builder { val: 10 };
  let x = 2;
  let y = 3;
  return b~>use_arg(x, y);
}
