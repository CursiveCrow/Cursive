# SPEC_COV: Pat-Dup-Err
// E-SEM-2713: Duplicate binding identifier within single pattern.
// Cannot bind the same identifier twice in one pattern.
record Point {
  x: i32,
  y: i32
}

public procedure main(ctx: Context) -> i32 {
  let p = Point { x: 1, y: 2 };
  let Point { x: a, y: a } = p;
  return 0i32;
}
