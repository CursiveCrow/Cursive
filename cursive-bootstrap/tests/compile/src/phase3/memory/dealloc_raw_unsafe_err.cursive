# SPEC_COV: DeallocRaw-Unsafe-Err
// E-MEM-3030: Raw heap deallocation outside unsafe block
public procedure main(ctx: Context) -> i32 {
  let raw: *imm i32 = null;
  let heap: $HeapAllocator = unsafe {
    transmute::<(*imm i32, *imm i32), $HeapAllocator>((raw, raw))
  };
  let p: *mut i32 = unsafe { heap~>alloc_raw(1usize) };
  heap~>dealloc_raw(p, 1usize);
  return 0;
}
