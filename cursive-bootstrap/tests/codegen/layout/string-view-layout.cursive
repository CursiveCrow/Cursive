// RUN: compile-pass
// SPEC: 6.1 Layout-String-View
// DESC: Verify string@View layout (ptr, len)

public procedure main(move ctx: Context) -> i32 {
    // string@View is a fat pointer: ptr (8) + len (8) = 16
    static_assert(sizeof(string@View) == 16)
    static_assert(alignof(string@View) == 8)

    return 0
}
