// RUN: compile-pass
// SPEC: 6.1 Layout-Range
// DESC: Verify range struct layout (start, end)

public procedure main(move ctx: Context) -> i32 {
    // Range<i32> contains (start: i32, end: i32)
    static_assert(sizeof(Range<i32>) == 8)
    static_assert(alignof(Range<i32>) == 4)

    // Range<i64> contains (start: i64, end: i64)
    static_assert(sizeof(Range<i64>) == 16)
    static_assert(alignof(Range<i64>) == 8)

    // Range<usize> for indexing
    static_assert(sizeof(Range<usize>) == 16)
    static_assert(alignof(Range<usize>) == 8)

    return 0
}
