// RUN: compile-pass
// SPEC: 6.1 Layout-Bytes-Managed
// DESC: Verify bytes@Managed layout (ptr, len, capacity)

public procedure main(move ctx: Context) -> i32 {
    // bytes@Managed has ptr (8) + len (8) + capacity (8) = 24
    static_assert(sizeof(bytes@Managed) == 24)
    static_assert(alignof(bytes@Managed) == 8)

    return 0
}
