// RUN: run-pass
// EXIT: 35
// SPEC: 6.5 Lower-Stmt-Assign
// DESC: Verify assignment lowering

record Point {
    x: i32
    y: i32
}

public procedure main(move ctx: Context) -> i32 {
    var x: i32 = 10
    x = 20
    x = x + 5

    var p: Point = Point { x: 0, y: 0 }
    p.x = 5
    p.y = 5

    return x + p.x + p.y
}
