// RUN: run-pass
// EXIT: 30
// SPEC: 6.4 Lower-Expr-Index-Array
// DESC: Verify array indexing lowering with bounds check

public procedure main(move ctx: Context) -> i32 {
    let arr: [i32; 5] = [10, 20, 30, 40, 50]

    let first: i32 = arr[0]
    let middle: i32 = arr[2]
    let last: i32 = arr[4]

    return if first == 10 && last == 50 { middle } else { 1 }
}
