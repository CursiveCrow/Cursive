// RUN: compile-pass
// SPEC: 6.1 Layout-String-Managed
// DESC: Verify string@Managed layout (ptr, len, capacity)

public procedure main(move ctx: Context) -> i32 {
    // string@Managed has ptr (8) + len (8) + capacity (8) = 24
    static_assert(sizeof(string@Managed) == 24)
    static_assert(alignof(string@Managed) == 8)

    return 0
}
