// RUN: run-pass
// EXIT: 42
// SPEC: 5.8
// DESC: String concatenation and length

public procedure main(move ctx: Context) -> i32 {
    let s1: string@View = "Hello, "
    let s2: string@View = "World!"
    let len1 = s1 ~> length()
    let len2 = s2 ~> length()
    let total: usize = len1 + len2
    if total == 13 {
        return 42
    }
    return 0
}
