// RUN: run-pass
// EXIT: 0
// SPEC: 5.10
// DESC: Enum with explicit discriminants

enum Status {
    Active = 1
    Inactive = 0
    Pending = 2
}

public procedure main(move ctx: Context) -> i32 {
    let s: Status = Status::Inactive
    let result: i32 = match s {
        Status::Active => { 1 }
        Status::Inactive => { 0 }
        Status::Pending => { 2 }
    }
    return result
}
