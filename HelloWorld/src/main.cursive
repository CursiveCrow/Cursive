/// C0X Feature Test - Procedure calls working

procedure identity(x: i32) -> i32 {
  return x
}

procedure add(x: i32, y: i32) -> i32 {
  return x + y
}

procedure compute(a: i32, b: i32, c: i32) -> i32 {
  var sum: i32 = add(a, b)
  return add(sum, c)
}

public procedure main(ctx: Context) -> i32 {
  var x: i32 = 10
  var y: i32 = 20
  var z: i32 = 12
  
  // 10 + 20 + 12 = 42
  return compute(x, y, z)
}
